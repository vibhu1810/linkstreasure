  courses = Object.keys(courses).map(function (key) { return courses[key]; });
  courses.sort(function(a, b) {
		var courseA = a.name.toUpperCase();
		var courseB = b.name.toUpperCase();
		return (courseA < courseB) ? -1 : (courseA > courseB) ? 1 : 0;
	});

</script>


